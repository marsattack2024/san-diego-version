# Production Environment Variables

# OpenAI API Key - Required for generating embeddings and chat completions
OPENAI_API_KEY="your-openai-api-key-here"

# Perplexity API Key for DeepSearch
PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Next.js environment variables
NEXT_PUBLIC_APP_URL=https://your-vercel-deployment-url.vercel.app

# Logging configuration
LOG_LEVEL=info
ENABLE_REMOTE_LOGGING=false 

# Supabase Configuration - Required for authentication and vector search
NEXT_PUBLIC_SUPABASE_URL="your-supabase-project-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_KEY="your-supabase-service-role-key"

# Production Configuration
NODE_ENV="production"
VERCEL_REGION="iad1"  # Set to your preferred Vercel region

# Fireworks AI API Key - Optional for alternative models
FIREWORKS_API_KEY="your-fireworks-api-key-here"

# Application Configuration
DEFAULT_AGENT="default"
ENABLE_DEEP_SEARCH=true
MAX_DOCUMENTS=10
SIMILARITY_THRESHOLD=0.5 

# Web Scraping Configuration
SCRAPER_ENDPOINT=your-scraper-endpoint-here

# Redis Configuration
REDIS_URL="URL"
   
# KV (Key-Value) specific configurations
KV_URL="URL"
KV_REST_API_URL="URL"
KV_REST_API_TOKEN="your-write-token"
KV_REST_API_READ_ONLY_TOKEN="your-read-only-token"